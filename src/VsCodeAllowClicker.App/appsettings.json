{
  "Target": {
    "ProcessNames": [ "Code", "Code - Insiders" ],
    "WindowTitleContains": "Visual Studio Code",
    "AdditionalWindowTitles": [ "Select an account", "Sign in", "Authorize", "GitHub" ]
  },
  "Matching": {
    "ButtonLabels": [ "Allow", "Continue", "Yes", "OK", "Authorize", "Accept", "Sign in" ],
    "PreClick": {
      "Enabled": false,
      "ControlType": "ListItem",
      "SelectionMode": "First",
      "Index": 0,
      "DelayAfterMs": 200
    },
    "WebviewFallback": {
      "Enabled": true,
      "TriggerWindowTitles": [ "Select an account", "GitHub", "Sign in", "Authorize" ],
      "KeySequence": [ "Enter" ],
      "DelayBetweenKeysMs": 100
    }
  },
  "SearchArea": {
    "Mode": "RightFraction",
    "RightFractionStart": 0.0,
    "NormalizedRect": { "X": 0.0, "Y": 0.0, "Width": 1.0, "Height": 1.0 }
  },
  "Polling": {
    "IntervalMs": 5000,
    "ClickThrottleMs": 1500
  },
  "Hotkey": {
    "Enabled": true,
    "Modifiers": [ "Ctrl", "Alt" ],
    "Key": "A"
  },
  "ConfirmHotkey": {
    "Enabled": true,
    "Modifiers": [ "Ctrl", "Alt" ],
    "Key": "C"
  },
  "UI": {
    "AutoStartEnabled": true,
    "ShowControlPanelOnStart": true,
    "ControlPanelPosition": "BottomLeft"
  }
}
